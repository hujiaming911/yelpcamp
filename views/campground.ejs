<%include partials/header%>



<div class="container">
      <div class="jumbotron" id="jumbotron">
        <h1>Welcome!</h1>
        <a class="btn btn-primary btn-large" href="/campground/new">Add new picture</a>
      </div>
      
      <div class="row text-center flex-wrap">
           <% for(var i=0;i<campground.length;i++){%>
          <div class="col-md-3 com-sm-6">
                <div class="thumbnail">
                    <img src="<%=campground[i].url%>">
                    <div id="caption">
                        <div class="caption">
                            <h4><%=campground[i].name%></h4>
                        </div>   
                        <p>
                            <a href="/campground/<%=campground[i]._id%>" class="btn btn-lg btn-primary">More info</a>
                        </p>
                    </div>
                </div>
           </div>
            <%}%>
      </div>
</div>
<%include partials/tail%>